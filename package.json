{
  "name": "outlook-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.7.0",
    "@testing-library/react": "^8.0.1",
    "bootstrap": "^4.3.1",
    "msal": "^1.0.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "xlsx": "^0.14.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 30
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "homepage": "https://narensulegai.github.io/outlook-client",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "jest-dom": "^3.4.0"
  }
}
